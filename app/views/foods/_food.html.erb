<div class="food-table" id="<%= dom_id food %>" >
  <p class="table-items">
    <%= food.name %>
  </p>

  <p class="table-items">
    <%= food.measurement_unit %>
  </p>

  <p class="table-items">
    $<%= food.price %>
  </p>

  <p class="table-items">
    <%= food.quantity %>units
  </p>

  <%= button_to "Destroy this food", user_food_url(user_id: @users.id, id: food.id), method: :delete %>
</div>
